# PRODUCTION .env Template
# Copy this file to .env and fill in real values for production

# BigQuery Configuration
BIGQUERY_LOGGING_ENABLED=true
BIGQUERY_PROJECT_ID=your-production-project-id

# Flask Configuration - PRODUCTION
FLASK_ENV=production
SECRET_KEY=your-super-secret-production-key-here
JWT_SECRET_KEY=your-jwt-secret-production-key-here

# Database - PRODUCTION
DATABASE_URL=postgresql://user:password@host:port/database

# Google Cloud Authentication - PRODUCTION
GOOGLE_APPLICATION_CREDENTIALS=/path/to/production-service-account.json

# Security Headers
FORCE_HTTPS=true
SESSION_COOKIE_SECURE=true
SESSION_COOKIE_HTTPONLY=true